{
  "name": "walnut-admin",
  "version": "0.0.1",
  "description": "An admin template, using `vue3`,`ts`,`vite`,`naive-ui`. Continuously updating...",
  "author": {
    "name": "zhaocl97",
    "email": "zhaocl97@foxmail.com",
    "url": "https://github.com/Zhaocl1997"
  },
  "homepage": "https://github.com/Zhaocl1997/walnut-admin-client",
  "scripts": {
    "predev": "esno build/generate/index.ts",
    "dev": "vite",
    "commit": "git-cz",
    "postinstall": "is-ci || husky install config/husky",
    "prebuild": "npm run clean:dist && npm run clean:cache && npm run gen:icon:copy && npm run gen:icon:list && npm run gen:icon:bundle",
    "build": "vite build",
    "build:stage": "esno build/stage.ts",
    "postbuild": "npm run gen:icon:recovery",
    "preview": "vite preview --port 8080",
    "release": "standard-version && git push --follow-tags origin naive-ui",
    "publish": "npm run predev && esno build/publish/index.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint:staged": "lint-staged --config config/lintstaged/.lintstagedrc.js",
    "lint:eslint": "eslint --config config/eslint/.eslintrc.js --ignore-pattern config/eslint/.eslintignore --fix",
    "lint:stylelint": "stylelint --config config/stylelint/.stylelintrc.js --fix",
    "lint:prettier": "prettier --config config/prettier/.prettierrc.js --write",
    "types:check": "vue-tsc --noEmit",
    "types:check:log": "esno build/types.ts",
    "clean:cache": "rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "clean:dist": "rimraf dist",
    "check:deps:update": "npm-check-updates",
    "gen:schema": "esno build/generate/genJSONSchemas.ts",
    "gen:icon:copy": "esno build/icon/hooks/copy.ts",
    "gen:icon:recovery": "esno build/icon/hooks/recovery.ts",
    "gen:icon:list": "esno build/icon/generate/list.ts",
    "gen:icon:bundle": "esno build/icon/generate/bundle.ts",
    "gen:icon:svg": "esno build/icon/generate/svg.ts"
  },
  "dependencies": {
    "@vueuse/core": "^7.6.2",
    "animate.css": "^4.1.1",
    "axios": "^0.26.0",
    "crypto-js": "^4.1.1",
    "easy-fns-ts": "1.1.0",
    "html-to-image": "^1.9.0",
    "sortablejs": "^1.14.0",
    "vue": "^3.2.31",
    "vue-i18n": "^9.2.0-beta.20",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@iconify/json": "^2.1.2",
    "@iconify/json-tools": "^1.0.10",
    "@iconify/tools": "^2.0.13",
    "@iconify/types": "^1.0.12",
    "@iconify/utils": "^1.0.23",
    "@iconify/vue": "^3.1.3",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^17.0.18",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@types/sortablejs": "^1.10.7",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@unocss/preset-icons": "^0.24.4",
    "@unocss/reset": "^0.24.4",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vitejs/plugin-vue-jsx": "^1.3.7",
    "@vue/compiler-sfc": "^3.2.31",
    "chalk": "^5.0.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.2.2",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.4.1",
    "esno": "^0.14.1",
    "fast-glob": "^3.2.11",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^12.3.4",
    "mockjs": "^1.1.0",
    "naive-ui": "^2.25.2",
    "npm-check-updates": "^12.3.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.2",
    "rollup-plugin-visualizer": "^5.5.4",
    "sass": "^1.49.7",
    "standard-version": "^9.3.2",
    "stylelint": "^14.5.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "^4.5.5",
    "typescript-json-schema": "^0.53.0",
    "unocss": "^0.24.4",
    "unplugin-auto-import": "^0.6.0",
    "unplugin-vue-components": "^0.17.18",
    "vfonts": "^0.1.0",
    "vite": "^2.8.3",
    "vite-plugin-banner": "^0.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.0.6",
    "vite-plugin-terminal": "^0.1.0",
    "vue-tsc": "^0.31.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/czcustomizable/.cz-config.js"
    }
  }
}
