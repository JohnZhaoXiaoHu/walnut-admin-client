{
  "name": "walnut-admin",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "commit": "git-cz",
    "postinstall": "is-ci || husky install config/husky",
    "prebuild": "npm run clean:dist && npm run clean:cache && npm run icon:copy && npm run icon:list && npm run icon:bundle",
    "build": "vite build",
    "build:stage": "esno build/stage.ts",
    "postbuild": "npm run icon:recovery",
    "serve": "vite preview",
    "release": "standard-version && git push --follow-tags origin main",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:staged": "lint-staged --config config/lintstaged/.lintstagedrc.js",
    "lint:eslint": "eslint --config config/eslint/.eslintrc.js --ignore-pattern config/eslint/.eslintignore --fix",
    "lint:stylelint": "stylelint --config config/stylelint/.stylelintrc.js --fix",
    "lint:prettier": "prettier --config config/prettier/.prettierrc.js --write",
    "types:check": "vue-tsc --noEmit --skipLibCheck",
    "types:check:log": "esno build/types.ts",
    "clean:cache": "rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "clean:dist": "rimraf dist",
    "icon:copy": "esno build/icon/index.ts --copy",
    "icon:list": "esno build/icon/list.ts",
    "icon:bundle": "esno build/icon/bundle.ts",
    "icon:recovery": "esno build/icon/index.ts --recovery"
  },
  "dependencies": {
    "@vueuse/core": "^4.8.1",
    "axios": "^0.21.1",
    "crypto-js": "^4.0.0",
    "easy-fns-ts": "^1.0.19",
    "element-plus": "^1.0.2-beta.40",
    "nprogress": "^0.2.0",
    "vue": "^3.0.11",
    "vue-i18n": "^9.1.4",
    "vue-router": "^4.0.6",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@iconify/json": "^1.1.328",
    "@iconify/json-tools": "^1.0.10",
    "@iconify/tools": "^1.3.17",
    "@iconify/vue": "^2.0.0-rc.3",
    "@intlify/vite-plugin-vue-i18n": "^2.1.2",
    "@types/crypto-js": "^4.0.1",
    "@types/mockjs": "^1.0.3",
    "@types/node": "^14.14.37",
    "@types/nprogress": "^0.2.0",
    "@types/rollup-plugin-visualizer": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vitejs/plugin-legacy": "^1.3.2",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.11",
    "chalk": "^4.1.0",
    "commitizen": "^4.2.3",
    "conventional-changelog-cli": "^2.1.1",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-vue": "^7.9.0",
    "esno": "^0.5.0",
    "fast-glob": "^3.2.5",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "lint-staged": "^10.5.4",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.3.0",
    "sass": "^1.32.8",
    "standard-version": "^9.2.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "typescript": "^4.2.3",
    "vite": "^2.2.1",
    "vite-plugin-components": "^0.8.4",
    "vite-plugin-compression": "^0.2.4",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-style-import": "^0.9.2",
    "vite-plugin-windicss": "^0.14.6",
    "vue-tsc": "^0.0.25"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/czcustomizable/.cz-config.js"
    }
  }
}
