{
  "name": "walnut-admin",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "commit": "git-cz",
    "postinstall": "is-ci || husky install config/husky",
    "prebuild": "npm run clean:dist && npm run clean:cache && npm run icon:copy && npm run icon:list && npm run icon:bundle",
    "build": "vite build",
    "build:stage": "esno build/stage.ts",
    "postbuild": "npm run icon:recovery",
    "serve": "vite preview",
    "release": "standard-version && git push --follow-tags origin main",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:staged": "lint-staged --config config/lintstaged/.lintstagedrc.js",
    "lint:eslint": "eslint --config config/eslint/.eslintrc.js --ignore-pattern config/eslint/.eslintignore --fix",
    "lint:stylelint": "stylelint --config config/stylelint/.stylelintrc.js --fix",
    "lint:prettier": "prettier --config config/prettier/.prettierrc.js --write",
    "types:check": "vue-tsc --noEmit --skipLibCheck",
    "types:check:log": "esno build/types.ts",
    "clean:cache": "rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "clean:dist": "rimraf dist",
    "icon:copy": "esno build/icon/index.ts --copy",
    "icon:list": "esno build/icon/list.ts",
    "icon:bundle": "esno build/icon/bundle.ts",
    "icon:recovery": "esno build/icon/index.ts --recovery",
    "update": "npm-check-updates"
  },
  "dependencies": {
    "@vueuse/core": "^6.5.3",
    "axios": "^0.23.0",
    "crypto-js": "^4.1.1",
    "easy-fns-ts": "^1.0.23",
    "nprogress": "^0.2.0",
    "vue": "^3.2.20",
    "vue-i18n": "^9.1.6",
    "vue-router": "^4.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@iconify/json": "^1.1.413",
    "@iconify/json-tools": "^1.0.10",
    "@iconify/tools": "^1.3.18",
    "@iconify/vue": "^3.0.0",
    "@intlify/vite-plugin-vue-i18n": "^2.4.0",
    "@types/crypto-js": "^4.0.2",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^16.10.4",
    "@types/nprogress": "^0.2.0",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-legacy": "^1.6.2",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/compiler-sfc": "^3.2.20",
    "chalk": "^4.1.2",
    "commitizen": "^3.0.0",
    "conventional-changelog-cli": "^2.1.1",
    "cz-customizable": "^5.5.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.19.1",
    "esno": "^0.10.1",
    "fast-glob": "^3.2.7",
    "husky": "^7.0.2",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.2.3",
    "mockjs": "^1.1.0",
    "naive-ui": "^2.19.7",
    "npm-check-updates": "^11.8.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "sass": "^1.42.1",
    "standard-version": "^9.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0",
    "typescript": "^4.4.4",
    "vfonts": "^0.1.0",
    "vite": "^2.6.7",
    "vite-plugin-components": "^0.12.1",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-html": "^2.1.1",
    "vite-plugin-windicss": "^1.4.11",
    "vue-tsc": "^0.28.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/czcustomizable/.cz-config.js"
    }
  }
}
