{
  "name": "walnut-admin",
  "version": "0.0.1",
  "description": "An admin template, using `vue3`,`ts`,`vite`,`naive-ui`. Continuously updating...",
  "author": {
    "name": "zhaocl97",
    "email": "zhaocl97@foxmail.com",
    "url": "https://github.com/Zhaocl1997"
  },
  "homepage": "https://github.com/Zhaocl1997/walnut-admin-client",
  "scripts": {
    "predev": "esno build/generate/index.ts",
    "dev": "vite",
    "commit": "git-cz",
    "postinstall": "is-ci || husky install config/husky",
    "prebuild": "npm run clean:dist && npm run clean:cache && npm run icon:copy && npm run icon:list && npm run icon:bundle",
    "build": "vite build",
    "build:stage": "esno build/stage.ts",
    "postbuild": "npm run icon:recovery",
    "serve": "vite preview",
    "release": "standard-version && git push --follow-tags origin naive-ui",
    "publish": "npm run predev && esno build/publish/index.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint:staged": "lint-staged --config config/lintstaged/.lintstagedrc.js",
    "lint:eslint": "eslint --config config/eslint/.eslintrc.js --ignore-pattern config/eslint/.eslintignore --fix",
    "lint:stylelint": "stylelint --config config/stylelint/.stylelintrc.js --fix",
    "lint:prettier": "prettier --config config/prettier/.prettierrc.js --write",
    "types:check": "vue-tsc --noEmit",
    "types:check:log": "esno build/types.ts",
    "clean:cache": "rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "clean:dist": "rimraf dist",
    "icon:copy": "esno build/icon/index.ts --copy",
    "icon:list": "esno build/icon/list.ts",
    "icon:bundle": "esno build/icon/bundle.ts",
    "icon:recovery": "esno build/icon/index.ts --recovery",
    "update": "npm-check-updates",
    "gen:schema": "esno build/generate/genJSONSchemas.ts"
  },
  "dependencies": {
    "@vueuse/core": "^7.5.3",
    "animate.css": "^4.1.1",
    "axios": "^0.25.0",
    "crypto-js": "^4.1.1",
    "easy-fns-ts": "^1.0.23",
    "sortablejs": "^1.14.0",
    "vue": "^3.2.22",
    "vue-i18n": "^9.2.0-beta.20",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@iconify/json": "^1.1.459",
    "@iconify/json-tools": "^1.0.10",
    "@iconify/tools": "^2.0.3",
    "@iconify/vue": "^3.1.2",
    "@types/crypto-js": "^4.1.0",
    "@types/node": "^17.0.10",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@types/sortablejs": "^1.10.7",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@unocss/preset-icons": "^0.22.5",
    "@vitejs/plugin-legacy": "^1.6.4",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "@vue/compiler-sfc": "^3.2.27",
    "chalk": "^5.0.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.2.2",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.3.0",
    "esno": "^0.14.0",
    "fast-glob": "^3.2.11",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^12.2.1",
    "mockjs": "^1.1.0",
    "naive-ui": "^2.24.1",
    "npm-check-updates": "^12.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.5.4",
    "sass": "^1.49.0",
    "standard-version": "^9.3.2",
    "stylelint": "^14.2.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "^4.5.4",
    "typescript-json-schema": "^0.53.0",
    "unocss": "^0.22.5",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-vue-components": "^0.17.13",
    "vfonts": "^0.1.0",
    "vite": "^2.7.13",
    "vite-plugin-banner": "^0.2.0",
    "vite-plugin-compression": "^0.4.0",
    "vite-plugin-html": "^2.1.2",
    "vite-plugin-terminal": "^0.0.12",
    "vite-plugin-windicss": "^1.6.3",
    "vue-tsc": "^0.30.6"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/czcustomizable/.cz-config.js"
    }
  }
}
