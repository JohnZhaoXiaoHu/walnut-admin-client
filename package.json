{
  "name": "walnut-admin",
  "version": "1.0.0",
  "description": "An admin template, using `vue3`,`ts`,`vite`,`naive-ui`. Continuously updating...",
  "author": {
    "name": "zhaocl97",
    "email": "zhaocl97@foxmail.com",
    "url": "https://github.com/Zhaocl1997"
  },
  "homepage": "https://github.com/Zhaocl1997/walnut-admin-client",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zhaocl1997/walnut-admin-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zhaocl1997/walnut-admin-client.git"
  },
  "scripts": {
    "predev": "esno build/generate/index.ts",
    "dev": "vite",
    "commit": "git-cz",
    "postinstall": "is-ci || husky install config/husky",
    "prebuild": "npm run clean:dist && npm run clean:cache && npm run gen:icon:copy && npm run gen:icon:list && npm run gen:icon:bundle",
    "build": "vite build",
    "build:stage": "esno build/stage.ts",
    "postbuild": "npm run gen:icon:recovery",
    "preview": "vite preview --port 8080",
    "release": "standard-version && git push --follow-tags origin naive-ui",
    "publish": "npm run predev && esno build/publish/index.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint:staged": "lint-staged --config config/lintstaged/.lintstagedrc.js",
    "lint:eslint": "eslint --config config/eslint/.eslintrc.js --ignore-pattern config/eslint/.eslintignore --fix",
    "lint:stylelint": "stylelint --config config/stylelint/.stylelintrc.js --fix",
    "lint:prettier": "prettier --config config/prettier/.prettierrc.js --write",
    "types:check": "vue-tsc --noEmit",
    "types:check:log": "esno build/types.ts",
    "clean:cache": "rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "clean:dist": "rimraf dist",
    "check:deps:update": "npm-check-updates",
    "gen:schema": "esno build/generate/genJSONSchemas.ts",
    "gen:icon:copy": "esno build/icon/hooks/copy.ts",
    "gen:icon:recovery": "esno build/icon/hooks/recovery.ts",
    "gen:icon:list": "esno build/icon/generate/list.ts",
    "gen:icon:bundle": "esno build/icon/generate/bundle.ts",
    "gen:icon:svg": "esno build/icon/generate/svg.ts"
  },
  "dependencies": {
    "@codemirror/autocomplete": "6.3.0",
    "@codemirror/commands": "6.1.2",
    "@codemirror/lang-javascript": "6.1.0",
    "@codemirror/search": "6.2.1",
    "@codemirror/theme-one-dark": "6.1.0",
    "@codemirror/view": "6.3.1",
    "@fingerprintjs/fingerprintjs": "3.3.6",
    "@tinymce/tinymce-vue": "5.0.0",
    "@vueuse/core": "9.3.0",
    "@vueuse/integrations": "9.3.0",
    "ali-oss": "6.17.1",
    "animate.css": "4.1.1",
    "axios": "1.1.3",
    "codemirror": "6.0.1",
    "cropperjs": "2.0.0-beta",
    "crypto-js": "4.1.1",
    "easy-fns-ts": "1.2.3",
    "echarts": "5.4.0",
    "highlight.js": "11.6.0",
    "html-to-image": "1.10.8",
    "intro.js": "6.0.0",
    "lodash-es": "4.17.21",
    "pinia": "2.0.23",
    "qrcode": "1.5.1",
    "signature_pad": "4.0.10",
    "socket.io-client": "^4.5.3",
    "sortablejs": "1.15.0",
    "tinymce": "6.2.0",
    "vue": "3.2.41",
    "vue-codemirror": "6.1.1",
    "vue-i18n": "9.2.2",
    "vue-router": "4.1.5",
    "vue-starport": "0.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@iconify/json": "2.1.122",
    "@iconify/json-tools": "1.0.10",
    "@iconify/tools": "2.1.0",
    "@iconify/types": "2.0.0",
    "@iconify/utils": "2.0.1",
    "@iconify/vue": "4.0.0",
    "@types/ali-oss": "6.16.5",
    "@types/codemirror": "5.60.5",
    "@types/crypto-js": "4.1.1",
    "@types/intro.js": "5.1.0",
    "@types/lodash-es": "4.17.6",
    "@types/node": "18.11.0",
    "@types/rollup-plugin-visualizer": "4.2.1",
    "@types/signature_pad": "2.3.2",
    "@types/sortablejs": "1.15.0",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "@unocss/preset-icons": "0.45.29",
    "@unocss/reset": "0.45.29",
    "@vitejs/plugin-legacy": "2.2.0",
    "@vitejs/plugin-vue": "3.1.2",
    "@vitejs/plugin-vue-jsx": "2.0.1",
    "@vue/compiler-sfc": "3.2.41",
    "chalk": "5.1.2",
    "commitizen": "4.2.5",
    "conventional-changelog-cli": "2.2.2",
    "cz-custom": "0.0.2",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-vue": "9.6.0",
    "esno": "0.16.3",
    "fast-glob": "3.2.12",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "javascript-obfuscator": "4.0.0",
    "lint-staged": "13.0.3",
    "mockjs": "1.1.0",
    "naive-ui": "2.33.5",
    "npm-check-updates": "16.3.11",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "rollup": "3.2.1",
    "rollup-plugin-obfuscator": "0.2.2",
    "rollup-plugin-visualizer": "5.8.3",
    "sass": "1.55.0",
    "standard-version": "9.5.0",
    "stylelint": "14.14.0",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-order": "5.0.0",
    "taze": "0.8.2",
    "typescript": "4.8.4",
    "typescript-json-schema": "0.54.0",
    "unocss": "0.45.29",
    "unplugin-auto-import": "0.11.2",
    "unplugin-vue-components": "0.22.8",
    "vfonts": "0.1.0",
    "vite": "3.1.8",
    "vite-plugin-banner": "0.5.0",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-html": "3.2.0",
    "vite-plugin-inspect": "0.7.5",
    "vite-plugin-mkcert": "1.10.1",
    "vite-plugin-restart": "0.2.0",
    "vite-plugin-terminal": "1.0.2",
    "vue-tsc": "1.0.8"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-custom"
    },
    "cz-custom": {
      "config": "config/cz-custom/.cz-config.js"
    }
  }
}
